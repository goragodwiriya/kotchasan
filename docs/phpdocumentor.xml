<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
>
    <title>phpDocumentor</title>
    <paths>
        <output>doc/</output>
    </paths>
    <version number="3.0.0">
        <api>
            <ignore hidden="true" symlinks="true">
                <path>projects/**/*</path>
                <path>PHPMailer/**/*</path>
                <path>PDF/**/*</path>
                <path>ckeditor/**/*</path>
                <path>tests/**/*</path>
            </ignore>
        </api>
    </version>
</phpdocumentor>
